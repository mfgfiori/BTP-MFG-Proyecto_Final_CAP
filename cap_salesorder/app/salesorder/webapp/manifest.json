{
    "_version": "1.49.0",
    "sap.app": {
        "id": "logaligroup.salesorder",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:lrop",
            "version": "1.9.2",
            "toolsId": "4f51c0eb-4eb9-4926-aef4-2adb084ebbd7"
        },
        "dataSources": {
            "mainService": {
                "uri": "/catalog/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "4.0"
                }
            }
        },
        "crossNavigation": {
            "inbounds": {
                "logaligroup-salesorder-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "Sales Orders",
                    "action": "display",
                    "title": "{{flpTitle}}",
                    "subTitle": "{{flpSubtitle}}",
                    "icon": ""
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.112.1",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.ushell": {},
                "sap.fe.templates": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "logaligroup.salesorder.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            }
        },
        "resources": {
            "css": []
        },
        "routing": {
            "config": {},
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "SalesOrderHeaderList",
                    "target": "SalesOrderHeaderList"
                },
                {
                    "pattern": "SalesOrderHeader({key}):?query:",
                    "name": "SalesOrderHeaderObjectPage",
                    "target": "SalesOrderHeaderObjectPage"
                },
                {
                    "pattern": "SalesOrderHeader({key})/to_Items({key2}):?query:",
                    "name": "SalesOrderItemObjectPage",
                    "target": "SalesOrderItemObjectPage"
                }
            ],
            "targets": {
                "SalesOrderHeaderList": {
                    "type": "Component",
                    "id": "SalesOrderHeaderList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "entitySet": "SalesOrderHeader",
                            "variantManagement": "Page",
                            "navigation": {
                                "SalesOrderHeader": {
                                    "detail": {
                                        "route": "SalesOrderHeaderObjectPage"
                                    }
                                }
                            }
                        }
                    }
                },
                "SalesOrderHeaderObjectPage": {
                    "type": "Component",
                    "id": "SalesOrderHeaderObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "editableHeaderContent": false,
                            "entitySet": "SalesOrderHeader",
                            "navigation": {
                                "to_Items": {
                                    "detail": {
                                        "route": "SalesOrderItemObjectPage"
                                    }
                                }
                            }
                        }
                    }
                },
                "SalesOrderItemObjectPage": {
                    "type": "Component",
                    "id": "SalesOrderItemObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "editableHeaderContent": false,
                            "entitySet": "SalesOrderItem"
                        }
                    }
                }
            }
        }
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "transactional"
    }
}